@inject SignInManager<ApplicationUser> SignInManager
@{
  var hasExternalLogins = (await SignInManager.GetExternalAuthenticationSchemesAsync()).Any();
}
<ul>
  <li><a @ManageNavPages.IndexNavClass(ViewContext) " id="profile" asp-page="./Index">Profile</a></li>
  <li><a @ManageNavPages.EmailNavClass(ViewContext) " id="email" asp-page="./Email">Email</a></li>
  <li><a @ManageNavPages.ChangePasswordNavClass(ViewContext) " id="change-password" asp-page="./ChangePassword">Password</a></li>
  @if (hasExternalLogins)
  {
    <li id="external-logins"><a id="external-login" class="nav-link @ManageNavPages.ExternalLoginsNavClass(ViewContext)" asp-page="./ExternalLogins">External logins</a></li>
  }
  <li><a class="nav-link @ManageNavPages.TwoFactorAuthenticationNavClass(ViewContext)" id="two-factor" asp-page="./TwoFactorAuthentication">Two-factor authentication</a></li>
  <li><a class="nav-link @ManageNavPages.PersonalDataNavClass(ViewContext)" id="personal-data" asp-page="./PersonalData">Personal data</a></li>
</ul>
